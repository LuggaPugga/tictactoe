{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"start": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".output/**", "dist/**"],
			"env": ["VITE_BACKEND_URL"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
